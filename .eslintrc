{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "arrow-parens": "off",
    "react/jsx-filename-extension": "off",
    "implicit-arrow-linebreak": "off"
  }
}
